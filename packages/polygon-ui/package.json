  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Rektangel/quadrilateral.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "react-component"
