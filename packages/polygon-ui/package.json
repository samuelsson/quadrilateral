    "@storybook/react": "^5.2.4",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.19",
    "@types/react": "^16.9.7",
    "@types/storybook__react": "^4.0.2",
    "babel-loader": "^8.0.6",
    "jest": "^24.9.0",
    "storybook-addon-styled-component-theme": "^1.2.5",
