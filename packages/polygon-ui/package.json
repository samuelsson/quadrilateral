{
  "name": "@samuelsson/polygon-ui",
  "version": "0.9.1",
  "description": "UI library for Quadrilateral",
  "author": "Erik Samuelsson",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/samuelsson/quadrilateral.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "stylelint ./src/**/*.scss && tsc --noEmit && eslint ./ --ext .ts,.tsx,.js",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint",
    "start": "start-storybook -p 6006"
  },
  "eslintConfig": {
    "extends": "@samuelsson/eslint-config"
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {
      "order/properties-alphabetical-order": null,
      "selector-class-pattern": "^[a-zA-Z0-9\\-]+$"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@samuelsson/eslint-config": "^1.6.2",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.7",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "css-loader": "^5.2.6",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass-loader": "^10.1.1",
    "storybook-dark-mode": "^1.0.8",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  }
}
